#include <stdio.h>
int main() {
    int R, n;
    char S[100];

    scanf("%d", &n);
    while (n--) {

        scanf("%d %20s", &R, S);

        for (int i = 0; S[i] != '\0'; i++) {
            for (int j = 0; j < R; j++) {
                printf("%c", S[i]);
            }
        }
        printf("\n");
    }
    return 0;
}
