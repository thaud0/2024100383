#include <stdio.h>
#include <string.h>
#include <stdlib.h>

int compare(const void *a, const void *b) {
    return (*(char *)b - *(char *)a);
}

int main() {
    char N[11];
    scanf("%s", N);

    int len = strlen(N);

    qsort(N, len, sizeof(char), compare);

    printf("%s\n", N);

    return 0;
}
